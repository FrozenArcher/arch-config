	email = yhy242989105@hotmail.com
	name = FrozenArcher
[init]
	defaultBranch = main
[core]
	editor = nvim
[alias]
	co = checkout
	br = branch
	c = commit
	s = status
	a = add
	pl = pull
	ps = push
[pull]
	rebase = false
[http]
	proxy = http://127.0.0.1:7890
[https]
	proxy = http://127.0.0.1:7890
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
